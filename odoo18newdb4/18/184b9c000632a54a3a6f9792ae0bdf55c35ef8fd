updateHistory(){const page=browser.location.href.replace(/^.*\/\/[^/]+/,"");const pageHistory=expirableStorage.getItem(HistoryService.HISTORY_STORAGE_KEY);const urlHistory=pageHistory?JSON.parse(pageHistory):[];if(!urlHistory.includes(page)){urlHistory.push(page);if(urlHistory.length>HistoryService.HISTORY_LIMIT){urlHistory.shift();}
