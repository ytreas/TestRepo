odoo.define('@im_livechat/embed/common/chatbot/chatbot_script_step_model',['@mail/core/common/record'],function(require){'use strict';let __exports={};const{Record}=require("@mail/core/common/record");const ChatbotScriptStep=__exports.ChatbotScriptStep=class ChatbotScriptStep extends Record{static id="id";static _name="chatbot.script.step";id;message;type;isLast=false;answers=Record.many("chatbot.script.answer");}
