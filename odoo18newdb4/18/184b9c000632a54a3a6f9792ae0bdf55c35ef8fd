odoo.define('@account/components/tests_shared_js_python/tests_shared_js_python',['@web/core/network/rpc','@web/core/registry','@account/helpers/account_tax','@odoo/owl'],function(require){'use strict';let __exports={};const{rpc}=require("@web/core/network/rpc");const{registry}=require("@web/core/registry");const{accountTaxHelpers}=require("@account/helpers/account_tax");const{xml,useState,Component}=require("@odoo/owl");const TestsSharedJsPython=__exports.TestsSharedJsPython=class TestsSharedJsPython extends Component{static template=xml`
        <button t-attf-class="#{state.done ? 'text-success' : ''}" t-on-click="processTests">Test</button>
    `;static props={tests:{type:Array,optional:true},};setup(){super.setup();this.state=useState({done:false});}
processTest(params){if(params.test==="taxes_computation"){let filter_tax_function=null;if(params.excluded_tax_ids&&params.excluded_tax_ids.length){filter_tax_function=(tax)=>!params.excluded_tax_ids.includes(tax.id);}
