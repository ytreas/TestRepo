odoo.define('@website_links/js/website_links',['@web/core/l10n/translation','@odoo/owl','@web/legacy/js/public/public_widget','@web/core/utils/ui','@web/core/browser/browser','@web/core/network/rpc','@web/core/utils/concurrency','@web_editor/js/core/owl_utils','@web/core/select_menu/select_menu','@web/core/utils/hooks','@web/core/dropdown/dropdown_item'],function(require){'use strict';let __exports={};const{_t}=require("@web/core/l10n/translation");const{Component,onWillStart,useState}=require("@odoo/owl");const publicWidget=require("@web/legacy/js/public/public_widget")[Symbol.for("default")];const{addLoadingEffect}=require('@web/core/utils/ui');const{browser}=require("@web/core/browser/browser");const{rpc}=require("@web/core/network/rpc");const{KeepLast}=require("@web/core/utils/concurrency");const{attachComponent}=require("@web_editor/js/core/owl_utils");const{SelectMenu}=require("@web/core/select_menu/select_menu");const{useService}=require("@web/core/utils/hooks");const{DropdownItem}=require("@web/core/dropdown/dropdown_item");class WebsiteLinksTagsWrapper extends Component{static template="website_links.WebsiteLinksTagsWrapper";static components={SelectMenu,DropdownItem};static props={placeholder:{optional:true,type:String},model:{optional:true,type:String},};setup(){this.orm=useService("orm");this.keepLast=new KeepLast();this.state=useState({placeholder:this.props.placeholder,choices:[],value:undefined,});onWillStart(async()=>{this.canCreateLinkTracker=await this.orm.call(this.props.model,"has_access",[[],"create"]);await this.loadChoice();});}
