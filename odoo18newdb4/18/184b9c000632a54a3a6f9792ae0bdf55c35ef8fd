            <div t-if="parts['detail'] and widget.displayDetail" class="flex-shrink-0 ms-auto">
                <t t-if="result['detail_strike']">
                    <span class="text-muted text-nowrap" style="text-decoration: line-through;">
                        <t t-out="result['detail_strike']"/>
                    </span>
                    <br/>
                </t>
                <b t-if="result['detail']" class="text-nowrap">
                    <t t-out="result['detail']"/>
                </b>
                <t t-if="result['detail_extra']">
                    <br/>
                    <span class="text-nowrap" t-out="result['detail_extra']"/>
                </t>
            </div>
        </div>
    </a>
    <t t-if="hasMoreResults">
        <button type="submit" class="dropdown-item text-center text-primary text-primary-emphasis">All results</button>
    </t>
</div>

`);
registerTemplateExtension("website.s_searchbar.autocomplete", `/website_event/static/src/snippets/s_searchbar/000.xml`, `<t t-inherit="website.s_searchbar.autocomplete" t-inherit-mode="extension" xml:space="preserve">
        <xpath expr="//div[@class='o_search_result_item_detail px-3']" position="inside">
            <span t-if="result['address_name']" class="small">
                <span class="fa fa-map-marker"/> <t t-out="result['address_name']"/>
            </span>
        </xpath>
    </t>

`);
                    });
