odoo.define('@web_tour/tour_service/tour_helpers',['@odoo/hoot-dom','@web/core/macro'],function(require){'use strict';let __exports={};const hoot=require("@odoo/hoot-dom");const{waitForStable}=require("@web/core/macro");const TourHelpers=__exports.TourHelpers=class TourHelpers{constructor(anchor){this.anchor=anchor;this.delay=20;}
async check(selector){const element=this._get_action_element(selector);await hoot.check(element);}
