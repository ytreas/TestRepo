odoo.define('@im_livechat/embed/common/chat_window_model_patch',['@im_livechat/embed/common/livechat_service','@mail/core/common/chat_window_model','@web/core/utils/patch'],function(require){'use strict';let __exports={};const{SESSION_STATE}=require("@im_livechat/embed/common/livechat_service");const{ChatWindow}=require("@mail/core/common/chat_window_model");const{patch}=require("@web/core/utils/patch");patch(ChatWindow.prototype,{setup(){super.setup();this.hasFeedbackPanel=false;},async close(){if(this.thread?.channel_type!=="livechat"){return super.close(...arguments);}
