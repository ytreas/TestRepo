wasBusAlreadyConnected=true;});bus_service.addEventListener("reconnect",()=>this.checkHasMissedNotifications());multi_tab.bus.addEventListener("shared_value_updated",({detail:{key}})=>{if(key==="bus.has_missed_notifications"){this.showOutdatedPageNotification();}});}
