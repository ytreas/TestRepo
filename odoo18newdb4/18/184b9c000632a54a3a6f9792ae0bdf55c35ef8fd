__exports.serializeComboItem=serializeComboItem;function serializeComboItem(comboItem){return{combo_item_id:comboItem.id,product_id:comboItem.product.id,no_variant_attribute_value_ids:comboItem.product.selectedNoVariantPtavIds,product_custom_attribute_values:comboItem.product.selectedCustomPtavs.map(customPtav=>({custom_product_template_attribute_value_id:customPtav.id,custom_value:customPtav.value,})),}}
