window.location='/shop/wishlist';},_onClickAddWish:function(ev){this._addNewProducts($(ev.currentTarget));},_onChangeVariant:function(ev){var $input=$(ev.target);var $parent=$input.closest('.js_product');var $el=$parent.find("[data-action='o_wishlist']");if(!this.wishlistProductIDs.includes(parseInt($input.val(),10))){$el.prop("disabled",false).removeClass('disabled').removeAttr('disabled');}else{$el.prop("disabled",true).addClass('disabled').attr('disabled','disabled');}
