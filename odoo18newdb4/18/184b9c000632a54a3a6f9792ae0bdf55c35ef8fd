odoo.define('@bus/outdated_page_watcher_service',['@web/core/browser/browser','@web/core/l10n/translation','@web/core/network/rpc','@web/core/registry'],function(require){'use strict';let __exports={};const{browser}=require("@web/core/browser/browser");const{_t}=require("@web/core/l10n/translation");const{rpc}=require("@web/core/network/rpc");const{registry}=require("@web/core/registry");const{DateTime}=luxon;const OutdatedPageWatcherService=__exports.OutdatedPageWatcherService=class OutdatedPageWatcherService{constructor(env,services){this.setup(env,services);}
