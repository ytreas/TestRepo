odoo.define('@im_livechat/embed/common/history_service',['@im_livechat/embed/common/expirable_storage','@web/core/browser/browser','@web/core/network/rpc','@web/core/registry'],function(require){'use strict';let __exports={};const{expirableStorage}=require("@im_livechat/embed/common/expirable_storage");const{browser}=require("@web/core/browser/browser");const{rpc}=require("@web/core/network/rpc");const{registry}=require("@web/core/registry");const HistoryService=__exports.HistoryService=class HistoryService{static HISTORY_STORAGE_KEY="im_livechat_history";static HISTORY_LIMIT=15;constructor(env,services){this.busService=services.bus_service;this.livechatService=services["im_livechat.livechat"];}
