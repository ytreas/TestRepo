				<t t-snippet="theme_mediquick.s_faq" t-thumbnail="/theme_mediquick/static/description/icon.png" />
			</snippets>
			<snippets class="o_panel_body" string="ContactUs Snippets">
				<t t-snippet="theme_mediquick.s_contactus" t-thumbnail="/theme_mediquick/static/description/icon.png" />
			</snippets>
