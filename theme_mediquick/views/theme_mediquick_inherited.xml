<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="theme_mediquick_snippets" inherit_id="website.snippets">
		<xpath expr="//snippets[@id='snippet_structure']" position="after">
		 	<snippets class="o_panel_body" string="Homepage Snippets">
				<t t-snippet="theme_mediquick.s_home_banner" t-thumbnail="/theme_mediquick/static/description/icon.png" />
