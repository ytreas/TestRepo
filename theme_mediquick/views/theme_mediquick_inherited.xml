				<t t-snippet="theme_mediquick.s_text_image" t-thumbnail="/theme_mediquick/static/description/icon.png" />
				<t t-snippet="theme_mediquick.s_teams" t-thumbnail="/theme_mediquick/static/description/icon.png" />
				<t t-snippet="theme_mediquick.s_gallery" t-thumbnail="/theme_mediquick/static/description/icon.png" />
				<t t-snippet="theme_mediquick.s_contact_us" t-thumbnail="/theme_mediquick/static/description/icon.png" />
			</snippets>
			<snippets class="o_panel_body" string="AboutUs Snippets">
				<t t-snippet="theme_mediquick.s_aboutus_banner" t-thumbnail="/theme_mediquick/static/description/icon.png" />
