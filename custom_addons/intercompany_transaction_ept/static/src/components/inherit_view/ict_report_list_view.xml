<?xml version="1.0" encoding="UTF-8" ?>
<templates>
  <t t-name="owl.IctReportListView.Buttons" owl="1">
    <xpath expr="//t[contains(@t-if, 'isExportEnable')]" position="before">
      <button class="btn btn-primary o_list_button_add" t-on-click="generateReport">Summary</button>
      <button class="btn btn-primary o_list_button_add button-gap" t-on-click="generateSheet">Excel</button>
      <button class="btn btn-primary o_list_button_add button-gap" t-on-click="showDatePicker">Filter</button>
    </xpath>
  </t>
</templates>
