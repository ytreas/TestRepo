    <record id="view_custom_vehicle_tree" model="ir.ui.view">
        <field name="name">custom.vehicle.type.tree</field>
        <field name="model">custom.vehicle.type</field>
        <field name="arch" type="xml">
            <tree string="Vehicle Master Data">
                <field name="code"/>
                <field name="name_en"/>
                <field name="name_np"/>
                <field name="time_duration"/>
                <field name="extra_charge"/>
                <field name="cost_per_hour"/>
                <field name="max_weight"/>
                <field name="vehicle_type"/>
            </tree>
        </field>
    </record>

    <record id="view_custom_vehicle_form" model="ir.ui.view">
        <field name="name">custom.vehicle.type.form</field>
        <field name="model">custom.vehicle.type</field>
        <field name="arch" type="xml">
            <form string="Vehicle Master Data">
                <sheet>
                    <group>
                        <field name="code" string="Vehicle Code"/>
                        <field name="name_en" string="Vehicle Name (English)"/>
                        <field name="name_np" string="Vehicle Name (Nepali)"/>
                        <field name="time_duration" string="Time Duration (Hr)"/>
                        <field name="extra_charge" string="Extra Charge (Rs)"/>
                        <field name="cost_per_hour" string="Cost Per Hour (Hr/Rs)"/>
                        <field name="max_weight" string="Max Weight (Kg)"/>
                        <field name="fine_amount"/>
                        <field name="vehicle_type" string="Vehicle Type"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_vehicle_type" model="ir.actions.act_window">
        <field name="name">Vehicle Types</field>
        <field name="res_model">custom.vehicle.type</field>
        <field name="view_mode">tree,form</field>
        <field name="target">current</field>
    </record>

    <menuitem id="menu_vehicle_types" name="Vehicle Configuration"  parent="menu_vehicle_details" sequence="1"/>                                                                                   " parent="menu_vehicle_details" sequence="1"/>
    <menuitem id="menu_vehicle_type" name="Vehicle Type" parent="menu_vehicle_types" action="action_vehicle_type"/>

