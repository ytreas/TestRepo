<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_cod" model="payment.provider">
        <field name="name">Cash on Delivery</field>
        <field name="code">custom</field>
        <field name="image_128" type="base64" file="tt_cod/static/description/icon.png"/>
        <field name="redirect_form_view_id" ref="payment_custom.redirect_form"/>
        <field name="custom_mode">cod</field>
        <field name="pending_msg" eval="False"/>
        <field name="is_published">True</field>
        <!-- <field name="payment_method_ids"
               eval="[Command.set([
                         ref('tt_cod.payment_method_cod'),
                     ])]"
        /> -->
    </record>

    <function model="payment.provider"
              name="_transfer_ensure_pending_msg_is_set"
              eval="[[ref('payment_provider_cod')]]"/>
</odoo>
