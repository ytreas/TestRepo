<?xml version="1.0" encoding="utf-8"?>
<templates>
   <t t-name="button_sale.ListView.Buttons" t-inherit="web.ListView.Buttons">
       <xpath expr="//div[hasclass('o_list_buttons')]" position="after">
           <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="OnTestClick">
               Print
           </button>
       </xpath>
   </t>
   <t t-name="button_sale.ListView.ButtonsB" t-inherit="web.ListView.Buttons">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="after">
            <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="OnTestClickB">
                Print
            </button>
        </xpath>
        <!-- <xpath expr="//div[hasclass('o_list_buttons')]" position="before">
            <div id="dynamic_info" style="margin-top: 10px; color: #007bff;">
              
               <span>No data yet</span>
           </div>
        </xpath> -->
        
    </t>
    <t t-name="firstprint" t-inherit="web.ListView.Buttons">
       <xpath expr="//div[hasclass('o_list_buttons')]" position="after">
           <button type="button" class="btn btn-primary" style="margin-left: 10px;"  t-on-click="firstprint">
               Print
           </button>
       </xpath>
   </t>
</templates>
