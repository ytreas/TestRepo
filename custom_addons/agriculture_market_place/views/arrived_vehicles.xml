        <field name="res_model">vehicle.number</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="vehicle_management.view_vehicle_number_tree"/>
        <!-- <field name="domain">[('date', 'not in', [False,None])] </field>  -->
